@model ThankYouViewModel
@{
    ViewBag.Title = @Model.PageTitle;
    Layout = "";
}
<link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @*<title>@ViewData["Title"] - MedicalManagementSystem</title>*@
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/site.css" />*@
    @*<link href="~/css/signin.css" rel="stylesheet" />*@

    <style>
        body {
            background: #222D32;
            font-family: 'Roboto', sans-serif;
        }

        .login-title {
            margin-top: 15px;
            text-align: left;
            font-size: 30px;
            letter-spacing: 2px;
            margin-top: 15px;
            font-weight: bold;
            color: #ECF0F5;
            padding-left: 150px;
        }

        .control-label {
            font-size: 15px;
            color: #b9b2b2;
            font-weight: bold;
            letter-spacing: 1px;
            padding-left: 150px;
        }

    </style>

    <title>@Model.PageTitle</title>
</head>
<body>
    <div class="container">
        <nav class="navbar  bg-dark navbar-dark">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a asp-action="index" asp-controller="home" class="nav-link">Home </a>
                </li>
                @*<li class="nav-item">
                        <a asp-action="index" asp-controller="home" class="nav-link">List </a>
                    </li>*@

            </ul>

            <ul class="navbar-nav ml-auto">
                <li class="nav-item">
                    <input type="hidden" id="useremail" name="useremail" value="@(Model.UserEmail = Model.UserEmail)" />
                    <input type="hidden" id="rolesname" name="rolesname" value="@(Model.RolesName = Model.RolesName)" />
                    <a class="nav-link" href="@Url.Action("usercontactdetails", "userregistrationcontroller", new { Model.UserEmail, Model.RolesName, Model.UserDetails})">Profile</a>
                </li>
            </ul>


        </nav>

    </div>
    <h2 class="login-title">@Model.PageTitle</h2>
    <div style="min-height: 300px;">
        
            <p class="control-label">
                @Model.Message
            </p>
        
    </div>
    </body>
</html>
